<?xml version="1.0" encoding="UTF-8"?>
<suite name="Coupons Automation Suite" verbose="10"
	parallel="tests" thread-count="1">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<!-- 1 -->

	<test name="add_New_Feed_With_Valid_Details">

		<classes>

			<class name="com.auction.verifications.Feeds">

				<methods>

					<include name="add_New_Feed_With_Valid_Details" />

				</methods>

			</class>

		</classes>

	</test>

	<!-- 2 -->

	<test name="add_New_Feed_Without_Entering_Feed_Name">

		<classes>

			<class name="com.auction.verifications.Feeds">

				<methods>

					<include name="add_New_Feed_Without_Entering_Feed_Name" />

				</methods>

			</class>

		</classes>

	</test>

	<!-- 3 -->

	<test name="add_New_Feed_Without_Entering_Slug">

		<classes>

			<class name="com.auction.verifications.Feeds">

				<methods>

					<include name="add_New_Feed_Without_Entering_Slug" />

				</methods>

			</class>

		</classes>

	</test>

	<!-- 4 -->

	<test name="add_New_Feed_With_Add_More_Custom_Replacements_Field">

		<classes>

			<class name="com.auction.verifications.Feeds">

				<methods>

					<include
						name="add_New_Feed_With_Add_More_Custom_Replacements_Field" />

				</methods>

			</class>

		</classes>

	</test>

	<!-- 5 -->

	<test name="edit_And_Save_Already_Added_Feed">

		<classes>

			<class name="com.auction.verifications.Feeds">

				<methods>

					<include name="edit_And_Save_Already_Added_Feed" />

				</methods>

