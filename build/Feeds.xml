			<class name="com.auction.verifications.Feeds">

				<methods>

					<include name="delete_Feed" />

				</methods>

			</class>

		</classes>

	</test>

	<!-- 7 -->

	<test name="edit_And_Save_Added_Custom_Replacements_Feed">

