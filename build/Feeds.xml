			<class name="com.auction.verifications.Feeds">

				<methods>

					<include name="delete_Added_Custom_Replacements_Feed" />

				</methods>

			</class>

		</classes>

	</test>

	<!-- 9 -->

	<test
		name="add_Feed_With_Entering_Alphabetical_Values_In_Items_per_page_Field">

		<classes>

			<class name="com.auction.verifications.Feeds">

				<methods>

					<include
						name="add_Feed_With_Entering_Alphabetical_Values_In_Items_per_page_Field" />


				</methods>

			</class>

		</classes>

	</test>

	<!-- 10 -->

	<test
		name="try_To_Add_New_Custom_Replacement_Field_With_Same_Value">

		<classes>

			<class name="com.auction.verifications.Feeds">

				<methods>

