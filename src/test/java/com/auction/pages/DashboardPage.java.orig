				+ LEGACY_ADMIN_LST_INPUT_AUCTIONEER + "']");
		common.select("//select[@id ='" + LEGACY_ADMIN_LST_INPUT_AUCTIONEER
				+ "']", LEGACY_ADMIN_LST_INPUT_AUCTIONEER_NAME);// select
																// auctioneer
		// sagar

		System.out.println("Step :: Select 100 From Page dropdown");
		common.log("<br></br> Step :: Select 100 From Page Dropdown");
		common.select(
				"//select[@id='" + LEGACY_ADMIN_LST_ITEMS_PER_PAGE + "']",
				"100");

		// common.waitForElement("//input[@value='Register to bid!']");

		if (common.isElementPresent("//table//td//a[text()='" + auctionName
				+ "']")) {
			System.out.println("auction " + auctionName
					+ " is present in this page");
			driver.findElement(
					By.xpath("//table//td//a[text()='" + auctionName
							+ "']/../..//input[@value='Register to bid!']"))
					.click();
			common.waitForAjax("0");

		} else {
			System.out.println("auction " + auctionName
					+ " is not  present in this page");

			driver.findElement(
					By.xpath("//span[@id='"
							+ LEGACY_ADMIN_PAGE_PAGINATION_WARNING_MESSAGE
							+ "']//a[text()='Next']")).click();

			common.pause(10);

			if (common.isElementPresent("//table//td//a[text()='" + auctionName
					+ "']")) {
				System.out.println("auction " + auctionName
						+ " is present in this page");
				driver.findElement(
						By.xpath("//table//td//a[text()='" + auctionName
								+ "']/../..//input[@value='Register to bid!']"))
						.click();
				common.pause(15);

			} else {
				System.out.println("auction " + auctionName
						+ " is not  present in this page");

				driver.findElement(
						By.xpath("//span[@id='"
								+ LEGACY_ADMIN_PAGE_PAGINATION_WARNING_MESSAGE
								+ "']//a[text()='Next']")).click();

				System.out.println("auction " + auctionName
						+ " is present in this page");
				driver.findElement(
						By.xpath("//table//td//a[text()='" + auctionName
								+ "']/../..//input[@value='Register to bid!']"))
						.click();
				common.pause(5);
			}

		}

		System.out.println("Step :: Click on Register to Bid !");

		common.pause(10);
		common.waitForElement("//ul[@id='messages']");
		System.out.println("Step :: Verify Success Message");
		common.log("<br></br> Step :: Verify Success Message");
		common.assertElementPresent("//ul[@id='messages']");

		common.jsClick(logout);

	}

