	/**
	 * 
	 * Verify auction status
	 * 
	 * @param name
	 * @param Auctiondate
	 * @param lots
	 * @param Format
	 * @param status
	 */

	public void VerifyAuctionStatus(String name, String Auctiondate, int lots,
			String Format, String status) {

		String totalitem = this.driver.findElement(
				By.xpath("//span[@id='" + LEGACY_ADMIN_TXT_TOTAL_ITEM_HEADER
						+ "']")).getText();
		int totalitem1 = Integer.parseInt(totalitem);
		int i;
		int item = 0;

		for (i = 0; i <= totalitem1;) {
			for (int j = 0; j < 100;) {
				String name1 = "//tr[@id='alf4row" + j + "']/td[4]";

				String str1 = this.driver.findElement(By.xpath(name1))
						.getText();// name

				String date1 = "//tr[@id='alf4row" + j + "']/td[5]";
				String str2 = this.driver.findElement(By.xpath(date1))
						.getText();// date
				str2 = str2.substring(0, 10);
				// System.out.println(str2);

				String lots1 = "//tr[@id='alf4row" + j + "']/td[6]";
				String str3 = this.driver.findElement(By.xpath(lots1))
						.getText();// lots

				String Format1 = "//tr[@id='alf4row" + j + "']/td[7]";
				String str4 = this.driver.findElement(By.xpath(Format1))
						.getText();// Format

				String status1 = "//tr[@id='alf4row" + j + "']/td[8]";
				String str5 = this.driver.findElement(By.xpath(status1))
						.getText();// status

				if (str1.equals(name) || str2.equals(Auctiondate)
						|| str3.equals(lots) || str4.equals(Format)
						|| str5.equals(status)) {
					common.log("<br></br> Step ::Verify Auction Status:");
					System.out.println(" Step ::Verify Auction Status:");
					// common.log(status);
					// System.out.println(status);
					item = 1;
					break;
				} else {
					String endstr = this.driver.findElement(
							By.xpath("//span[@id='endResultHeader']"))
							.getText();
					int endstr1 = Integer.parseInt(endstr);

					String start = this.driver.findElement(
							By.xpath("//span[@id='startResultHeader']"))
							.getText();
					int start1 = Integer.parseInt(start);
					int f = endstr1 - start1;
					// System.out.println(f);
					if (j == f) {
						if (i == totalitem1) {
							System.out.println("Element not found");
							common.log("<br></br>Step :: Element not found");
							item = 1;
							break;
						} else {
							driver.findElement(
									By.xpath("//span[@id='c3']//a[text()='Next']"))
									.click();
							j = 0;
							String end = this.driver.findElement(
									By.xpath("//span[@id='endResultHeader']"))
									.getText();
							int end1 = Integer.parseInt(end);
							i = end1;
						}
					} else {
						j++;
					}
				}

			}

			if (item == 1) {
				break;
			} else {
				String end = this.driver.findElement(
						By.xpath("//span[@id='endResultHeader']")).getText();
				int end1 = Integer.parseInt(end);
				i = end1;
			}
		}

	}

