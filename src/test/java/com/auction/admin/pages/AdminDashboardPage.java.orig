	/**
	 * Search Lot
	 * 
	 * @param lot
	 * @param na
	 * @throws ParseException
	 * @throws InterruptedException
	 */
	public void searchlot(String lot, String na) throws ParseException,
			InterruptedException {

		String totalitem = this.driver.findElement(
				By.xpath("//span[@id='" + LEGACY_ADMIN_TXT_TOTAL_ITEM_HEADER
						+ "']")).getText();
		int totalitem1 = Integer.parseInt(totalitem);
		int i;
		int item = 0;

		for (i = 0; i <= totalitem1;) {

			for (int j = 0; j < 100;) {
				String id = "//*[@id='alf5row" + j + "']/td[2]";
				String str1 = this.driver.findElement(By.xpath(id)).getText();// name
				String name = "//*[@id='alf5row" + j + "']/td[5]";
				String str2 = this.driver.findElement(By.xpath(name)).getText();// name
				if (str1.equals(lot)) {
					if (str2.equals(na)) {
						common.log("<br></br> Step ::Lot Display properly");
						String category = driver.findElement(
								By.xpath("//*[@id='alf5row" + j + "']/td[5]"))
								.getText();
						System.out.println("Application display category as :"
								+ category);
						item = 1;
						break;
					}
				} else {
					String endstr = this.driver.findElement(
							By.xpath("//*[@id='endResultHeader']")).getText();
					int endstr1 = Integer.parseInt(endstr);

					String start = this.driver.findElement(
							By.xpath("//*[@id='startResultHeader']")).getText();
					int start1 = Integer.parseInt(start);
					int f = endstr1 - start1;
					System.out.println(f);

					if (j == f) {
						if (i == totalitem1) {
							System.out.println("lot Not Visible");
							common.log("<br></br>Step :: lot Not Visible");
							item = 1;
							break;
						} else {
							driver.findElement(
									By.xpath("//*[@id='"
											+ LEGACY_ADMIN_TXT_USER_NAME
											+ "']/span[5]")).click();
							j = 0;
							String end = this.driver.findElement(
									By.xpath("//*[@id='endResultHeader']"))
									.getText();
							int end1 = Integer.parseInt(end);
							i = end1;
						}
					} else {
						j++;
					}
				}

			}

			if (item == 1) {
				break;
			}
		}

	}

