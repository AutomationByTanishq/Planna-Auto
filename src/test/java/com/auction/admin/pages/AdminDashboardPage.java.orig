	public void Verify_Registered_User_Field(String EmailUser, String signUpUser)

	{

		System.out.println("Step :: Click on Users Tab");
		common.log("<br></br> Step :: Click on Users Tab");
		driver.findElement(By.xpath("//a[@title='Users']")).click();

		System.out.println("Step :: Fill User Name");
		common.log("<br></br> Step :: Fill User Name");
		driver.findElement(By.xpath("//input[@id='ulf3']"))
				.sendKeys(signUpUser);

		System.out.println("Step :: Click on Search Button");
		common.log("<br></br> Step :: Click on Search Button");
		driver.findElement(By.xpath("//input[@id='ulf4']")).click();
		common.pause(15);

		System.out.println("Step :: Click on Edit Icon");
		common.log("<br></br> Step :: Click Edit Icon");
		driver.findElement(
				By.xpath("//table[@id='ulf10']//td[text()='" + signUpUser
						+ "']//..//a[@title='Edit']")).click();
		common.pause(10);

		String verifyusername = common.getValue("//*[@id='uip1']");

		String verifyfirstname = common.getValue("//*[@id='uip6']");

		String verifylastname = common.getValue("//*[@id='uip1']");

		String verifyphonenumber = common.getValue("//*[@id='pnuip20']");

		common.assertTwoValuesAreEqual(verifyfirstname, signUpUser);
		common.assertTwoValuesAreEqual(verifylastname, signUpUser);
		common.assertTwoValuesAreEqual(verifyusername, signUpUser);
		common.assertTwoValuesAreEqual(verifyphonenumber,
				LEGACY_USER_SIGNUP_TXT_PHONE_VALUE);

	}

