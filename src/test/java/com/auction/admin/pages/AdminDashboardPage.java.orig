				String status1 = "//tr[@id='alf4row" + j + "']/td[8]";
				String str5 = this.driver.findElement(By.xpath(status1))
						.getText();// status

				if (str1.equals(name)) {
					if (str2.equals(Auctiondate) || str3.equals(lots)
							|| str4.equals(Format) || str5.equals(status)) {
						common.log("<br></br> Step ::Verify Auction Visiblity.");

						System.out.println("Auction is Visible");
						item = 1;
						break;
					}
				} else {
					String endstr = this.driver.findElement(
							By.xpath("//span[@id='endResultHeader']"))
							.getText();
					int endstr1 = Integer.parseInt(endstr);

					String start = this.driver.findElement(
							By.xpath("//span[@id='startResultHeader']"))
							.getText();
					int start1 = Integer.parseInt(start);
					int f = endstr1 - start1;
					System.out.println(f);

					if (j == f) {
						if (i == totalitem1) {
							System.out.println("Auction Not Visible");
							common.log("<br></br>Step :: Auction Not Visible");
							item = 1;
							break;
						} else {
							driver.findElement(
									By.xpath("//*[@id='"
											+ LEGACY_ADMIN_TXT_USER_NAME
											+ "']/span[5]")).click();
							j = 0;
							String end = this.driver.findElement(
									By.xpath("//span[@id='endResultHeader']"))
									.getText();
							int end1 = Integer.parseInt(end);
							i = end1;
						}
					} else {
						j++;
					}
				}

			}

			if (item == 1) {
				break;
			}
		}

	}

